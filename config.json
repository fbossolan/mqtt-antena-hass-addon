{
  "name": "MQTT Antena",
  "version": "1.5.8",
  "slug": "mqtt_antena",
  "description": "A web UI for monitoring and publishing MQTT messages, ideal for development and debugging.",
  "arch": ["aarch64", "amd64", "armhf", "armv7", "i386"],

  "startup": "application",
  "boot": "auto",

  "ingress": true,
  "ingress_only": true,
  "ingress_port": 8585,

  "panel_title": "MQTT Antena",
  "panel_icon": "mdi:video-input-antenna",
  "panel_admin": false,

  "image": "fbossolan/mqtt-antena",

  "map": ["addons"],

  "environment": {
    "SECRET_KEY": "{secret_key}"
  },

  "options": {
    "secret_key": ""
  },

  "schema": {
    "secret_key": "password"
  }
}
